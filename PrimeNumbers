def prime(n):
    no=[]
    for i in range(1,n+1):
        if n%i==0:
            no.append(i)
        else:
            pass
    if len(no)==2:
        print(n,"is a Prime number, because it has only two multiples:",no)
    else:
        print(n,"is not a prime number, because it has more than two multiples:",no)
